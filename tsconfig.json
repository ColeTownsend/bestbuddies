{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "src",
    "db",
    "astro.config.ts"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "allowJs": true,
    "lib": [
      "es2022",
      "dom",
      "dom.iterable"
    ],
    "strictNullChecks": true
  }
}